{"_id":"qTJIwDTeVJqm9RCg","name":"Felsine Battle-Horn","type":"weapon","img":"systems/pf2e/icons/spells/bullhorn.webp","data":{"description":{"value":"<p>This heavy reinforced musical horn is made of the hollowed horn of a large beast and is decorated with the clan sign of its owner.</p>\n<hr />\n<p><strong>Special:</strong> A Felsine Battle-Horn acts as a handheld instrument.</p>"},"source":{"value":"Eldritch Ancestries: Felsine"},"traits":{"value":["hb_felsine","forceful","shove"],"rarity":{"value":"uncommon"},"custom":""},"rules":[],"slug":null,"schema":{"version":0.692,"lastMigration":{"datetime":"2021-11-20T02:50:59.965-05:00","version":{"schema":0.669,"foundry":"0.8.9","system":"2.19.0.9009"}}},"quantity":{"value":1},"baseItem":null,"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"-"},"equippedBulk":{"value":""},"price":{"value":"3 gp"},"equipped":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":null},"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"collapsed":{"value":false},"size":{"value":"med"},"identification":{"status":"identified","unidentified":{"name":"Unusual Weapon","img":"systems/pf2e/icons/unidentified_item_icons/weapon.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":0},"invested":{"value":null},"category":"advanced","group":"club","hands":{"value":false},"bonus":{"value":0},"damage":{"value":"","dice":1,"die":"d10","damageType":"bludgeoning"},"bonusDamage":{"value":0},"splashDamage":{"value":0},"range":null,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":0},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":null},"propertyRune2":{"value":null},"propertyRune3":{"value":null},"propertyRune4":{"value":null},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDi